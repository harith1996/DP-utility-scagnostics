{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
        "url": "collected_data_Rscag=09_07.csv"
    },
    "transform": [
        {
            "calculate": "'https://raw.githubusercontent.com/harith1996/dp-utility-scagnostics/dev/backend/datasets/images/' + datum.datasetName + '.png'",
            "as": "image"
        }
    ],
    "vconcat": [
        {
            "height": 400,
            "width": 500,
            "mark": {
                "type": "line",
                "tooltip": true
            },
            "encoding": {
                "x": {
                    "field": "epsilon",
                    "type": "ordinal"
                },
                "y": {
                    "aggregate": "mean",
                    "field": "rmsd",
                    "type": "quantitative",
                    "scale": {
                        "domain": [
                            0,
                            1.4
                        ]
                    }
                },
                "color": {
                    "field": "algorithm",
                    "type": "nominal"
                },
                "strokeDash": {
                    "field": "denoiseLevel",
                    "type": "nominal"
                }
            }
        },
        {
            "height": 400,
            "width": 500,
            "mark": {
                "type": "line"
            },
            "encoding": {
                "x": {
                    "field": "denoiseLevel",
                    "title": "Denoise Level",
                    "type": "nominal"
                },
                "y": {
                    "aggregate": "mean",
                    "field": "rmsd",
                    "type": "quantitative",
                    "scale": {
                        "domain": [
                            0,
                            2
                        ]
                    }
                },
                "color": {
                    "field": "algorithm",
                    "type": "nominal"
                }
            }
        },
        {
            "height": 400,
            "width": 500,
            "mark": {
                "type": "bar"
            },
            "encoding": {
                "x": {
                    "field": "datasetName",
                    "type": "nominal"
                },
                "y": {
                    "aggregate": "mean",
                    "field": "Clumpy_og",
                    "type": "quantitative",
                    "scale": {
                        "domain": [
                            0,
                            1
                        ]
                    }
                },
                "xOffset": {
                    "field": "denoiseLevel"
                },
                "opacity": {
                    "field": "denoiseLevel",
                    "type": "nominal"
                },
                "tooltip": [
                    {
                        "field": "image"
                    }
                ]
            }
        }
    ]
}