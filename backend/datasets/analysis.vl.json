{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
        "url": "./collected_data_new_datasets_date=29_06.csv"
    },
    "transform": [
        {
            "fold": [
                "rmsd_og_denoised_priv",
                "rmsd_og_denoised_convHull"
            ],
            "as": [
                "extent",
                "rmsd"
            ]
        },{
            "fold": [
                "clumpyScore_diff_og_priv",
                "clumpyScore_og_denoised_priv"
            ],
            "as": [
                "noise_state",
                "clumpyScore_diff"
            ]
        },
        {
            "filter": "datum.unbinThreshold == 0 || datum.unbinThreshold == 1 || datum.unbinThreshold == 2 || datum.unbinThreshold == 3 || datum.unbinThreshold == 4"
        }
    ],
    "vconcat": [
        {
            "height": 400,
            "width": 500,
            "mark": {
                "type": "line"
            },
            "encoding": {
                "x": {
                    "field": "epsilon",
                    "type": "ordinal"
                },
                "y": {
                    "aggregate": "variance",
                    "field": "clumpyScore_diff",
                    "type": "quantitative",
                    "scale": {
                        "domain": [
                            0,
                            0.5
                        ]
                    }
                },
                "color": {
                    "field": "algorithm",
                    "type": "nominal"
                },
                "strokeDash": {
                    "field": "noise_state",
                    "type": "nominal"
                }
            }
        },
        {
            "height": 400,
            "width": 500,
            "mark": {
                "type": "line"
            },
            "encoding": {
                "x": {
                    "field": "unbinThreshold",
                    "title": "Denoise Level",
                    "type": "nominal"
                },
                "y": {
                    "aggregate": "mean",
                    "field": "clumpyScore_diff",
                    "type": "quantitative",
                    "scale": {
                        "domain": [
                            0,
                            1
                        ]
                    }
                },
                "color": {
                    "field": "algorithm",
                    "type": "nominal"
                },
                "strokeDash": {
                    "field": "noise_state",
                    "type": "nominal"
                }
            }
        },
        {
            "height": 400,
            "width": 500,
            "mark": {
                "type": "bar"
            },
            "encoding": {
                "x": {
                    "field": "dataset",
                    "type": "nominal"
                },
                "y": {
                    "aggregate": "mean",
                    "field": "clumpyScore_diff_og_convHull",
                    "type": "quantitative",
                    "scale": {}
                }
            }
        }
    ]
}